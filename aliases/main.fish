# Confirm before overwriting files
alias cp="cp -i"
alias mv='mv -i'
alias rm='rm -i'

# Better finging files
alias f="find . | grep "

# Set Autojump to j
alias j="z"

# Human readable flags
alias df="df -h"
alias free="free -h"

# Rickroll
alias rickroll="curl -s -L https://raw.githubusercontent.com/keroserene/rickrollrc/master/roll.sh | bash"
